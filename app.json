{
	"expo": {
		"name": "wedding-service",
		"slug": "wedding-service",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./shared/images/adaptive-icon.png",
		"scheme": "myapp",
		"userInterfaceStyle": "automatic",
		"newArchEnabled": true,
		"ios": {
			"supportsTablet": true,
			"bundleIdentifier": "com.dovran-orazov.weddingservice"
		},
		"android": {
			"enableDangerousExperimentalLeanBuilds": true,
			"adaptiveIcon": {
				"foregroundImage": "./shared/images/adaptive-icon.png",
				"backgroundColor": "#ffffff"
			},
			"package": "com.dovran_orazov.weddingservice"
		},
		"web": {
			"bundler": "metro",
			"output": "static",
			"favicon": "./shared/images/favicon.png"
		},
		"plugins": [
			"expo-router",
			"expo-video",
			"expo-secure-store"
		],
		"experiments": {
			"typedRoutes": true
		},
		"extra": {
			"API_BASE_URL": "https://ybady.com.tm:8090/api/v1",
			"router": {
				"origin": false
			},
			"eas": {
				"projectId": "80ba94ef-cc2b-4442-8591-0533656636e4"
			}
		}
	}
}
